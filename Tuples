#Add an item

a=(4,6,2,8,3,1)
print(a)
a=a+(9,)
print(a)
a=a[:5]+(15,20,25)+a[:5]
print(a)
b=list(a)
b.append(30)
a=tuple(b)
print(a)


#Length of a tuple

a=('c','y','f','u','n','o')
print(a)
print(len(a))


#Reverse a Tuple

a=('c','y','f','u','n','o')
print(a[::-1])


#Repeated element in a tuple

a=(2,4,5,6,2,3,4,4,7)
b=int(input())
print(a)
print(a.count(b))
